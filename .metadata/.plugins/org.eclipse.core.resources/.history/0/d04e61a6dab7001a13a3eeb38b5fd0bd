import java.util.*;

public class Magazine
{//start class
	public static void main(String[] args)
	{//start main
		int gen1 = 0, gen2 = 0, gen3 = 0;
		String[] mag = new String[13];
		gen1 = RNG1(gen1,mag);
		gen2 = RNG2(gen2, gen1, mag);
		gen3 = RNG3(gen3, gen1, gen2, mag);
		System.out.println(gen1);
		System.out.println(gen2);
		for(int i =0; i < mag.length; i++)
		{//start loop
			if(i != gen1 && i !=gen2 && i != gen3)
			{
				System.out.println("Reg");
			}
			else
			{
				System.out.println("Lucky");
			}
		}//end loop
		
	}//end main
	public static int RNG1 (int x, String[] mag)//makes RNG number
	{//start method
		Random rand = new Random();
		x = rand.nextInt(mag.length);
		return x;
	}//end method
	public static int RNG2 (int x, int y, String[] mag)
	{//start method
		Random rand = new Random();
		x = rand.nextInt(mag.length);
		if (x == y || x == y+1)
		{
			x = rand.nextInt(mag.length);
		}
		return x;
	}//end method
	public static int RNG3 (int x, int y, int z, String[] mag)
	{//start method
		Random rand = new Random();
		if(x == y || x == z || x == y+1)
		{
			x = rand.nextInt(mag.length);
		}
		return x;
	}//end method

}//end class
